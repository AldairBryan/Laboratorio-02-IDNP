<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Apellido Paterno:"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="111dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombres:"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="291dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Colegio de Procedencia:"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="486dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fecha de Nacimiento:"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="396dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Carrera a la que postula"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="586dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Apellido Materno:"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="203dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="Ingrese sus datos"
        android:textColor="#000000"
        android:textSize="23sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="40dp" />

    <EditText
        android:id="@+id/inputApellidoP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="167dp"
        tools:layout_editor_absoluteY="93dp" />

    <EditText
        android:id="@+id/inputApellidoM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="183dp" />

    <EditText
        android:id="@+id/inputNombres"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="273dp" />

    <EditText
        android:id="@+id/inputFechaNacimiento"
        android:layout_width="152dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteX="225dp"
        tools:layout_editor_absoluteY="383dp" />

    <EditText
        android:id="@+id/inputCodigoP"
        android:layout_width="148dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="228dp"
        tools:layout_editor_absoluteY="469dp" />

    <Spinner
        android:id="@+id/inputCarrera"
        android:layout_width="150dp"
        android:layout_height="34dp"
        tools:layout_editor_absoluteX="227dp"
        tools:layout_editor_absoluteY="577dp" />

</androidx.constraintlayout.widget.ConstraintLayout>